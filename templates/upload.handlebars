<h2>Upload Photo</h2>

<form method="post" action="/photo/upload">
    <label>Photo Name:</label>
    <input type="text" name="photoName" required>

    <label>Album:</label>
    <select name="albumId">
        {{#each albums}}
            <option value="{{this.id}}">{{this.name}}</option>
        {{/each}}
    </select>

    <button type="submit">Upload</button>
</form>
